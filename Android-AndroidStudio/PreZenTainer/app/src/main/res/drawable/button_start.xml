<?xml version="1.0" encoding="utf-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:state_pressed="true">
		<shape>
			<corners
				android:radius="48dp" />
			<solid android:color="@color/clickstate" />
		</shape>
	</item>
	<item
		android:state_enabled="false">
		<shape>
			<corners
				android:radius="48dp" />
			<solid android:color="@color/cpb_grey" />
		</shape>
	</item>
	<item>
		<shape>
			<corners
				android:radius="48dp" />
			<solid android:color="@color/idle" />
		</shape>
	</item>
</selector>


