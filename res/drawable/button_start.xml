<?xml version="1.0" encoding="utf-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:state_pressed="true">
		<shape>
			<corners
				android:radius="10dp" />
			<solid android:color="@color/indigo100" />
		</shape>
	</item>
	<item
		android:state_enabled="false">
		<shape>
			<corners
				android:radius="10dp" />
			<solid android:color="#E1F5FE" />
		</shape>
	</item>
	<item>
		<shape>
			<corners
				android:radius="10dp" />
			<solid android:color="@color/lightblue300" />
		</shape>
	</item>
</selector>


